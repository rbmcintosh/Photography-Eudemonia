{
  "name": "minify-images",
  "version": "1.0.6",
  "main": "index.js",
  "repository": "https://github.com/nielse63/minify-images.git",
  "issues": "https://github.com/nielse63/minify-images/issues",
  "author": "Erik Nielsen <erik@312development.com>",
  "homepage": "https://github.com/nielse63/minify-images",
  "license": "MIT",
  "engines": {
    "node": ">=7.4.0",
    "npm": ">=4.0.5"
  },
  "files": [
    "lib/",
    "index.js"
  ],
  "keywords": [
    "imagemin",
    "imagemin-mozjpeg",
    "imagemin-pngquant",
    "imagemin-svgo",
    "minify",
    "compress",
    "image",
    "images",
    "jpeg",
    "jpg",
    "png",
    "svg"
  ],
  "scripts": {
    "lint": "eslint ./*.js src/ test/ --fix",
    "prebuild": "rm -rf lib/; eslint src/ --fix",
    "build": "NODE_ENV=production babel src/ -d lib",
    "pretest": "rm -rf *.log coverage/; npm run build",
    "test": "NODE_ENV=test nyc --reporter=html mocha ./test/index.js",
    "posttest": "rm -rf test/dest *.log",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-polyfill": "^6.20.0",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "imagemin-svgo": "^5.2.0",
    "is-glob": "^3.1.0",
    "is-plain-object": "^2.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "chai-asserttype": "^1.0.5",
    "coveralls": "^2.11.15",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "nyc": "^10.1.2"
  }
}
